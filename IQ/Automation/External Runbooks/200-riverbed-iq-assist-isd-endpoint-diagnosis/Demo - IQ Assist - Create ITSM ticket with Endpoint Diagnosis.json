                "description": "",
                "properties": {
                    "x": 351,
                    "y": 435,
                    "comment": "<p dir=\"ltr\" class=\"editor-paragraph\"><b><strong style=\"white-space: pre-wrap;\" class=\"editor-text-bold\">Payload example</strong></b></p><p class=\"editor-paragraph\"><span style=\"white-space: pre-wrap;\">{</span></p><p dir=\"ltr\" class=\"editor-paragraph\"><span style=\"white-space: pre-wrap;\">\"device_name\": \"Adam_Covert_WKS\",</span></p><p dir=\"ltr\" class=\"editor-paragraph\"><span style=\"white-space: pre-wrap;\">\"alert_name\": \"Short Description\",</span></p><p dir=\"ltr\" class=\"editor-paragraph\"><span style=\"white-space: pre-wrap;\">\"alert_event_name\": \"Description\"</span></p><p class=\"editor-paragraph\"><span style=\"white-space: pre-wrap;\">}</span></p>",
                    "debug": false
